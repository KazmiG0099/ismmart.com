{
  "builds": [
    {
      "src": "ecommerce/wsgi.py",
      "use": "@vercel/python"
      "config": { "maxLambdaSize":"15mb", "runtime":"python3.11" }

    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "ecommerce/wsgi.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11",
    "PYTHONUNBUFFERED": "1",
    "DJANGO_SETTINGS_MODULE": "ecommerce.settings"
  },
  "functions": {
    "vc__handler__python": {
      "runtime": "vercel-python@0.24.0"
    }
  }
}
